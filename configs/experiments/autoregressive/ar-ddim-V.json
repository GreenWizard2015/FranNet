{
  "inherit": "ar-ddim-extended.json",
  "experiment": {
    "title": "Autoregressive DDIM with V objective",
    "description": "DDIM-V with autoregressive model"
  },
  "model": {
    "restorator": {
      "inherit": "configs/models/restorator/autoregressive.json",
      "sampler": {
        "name": "DDIM",
        "interpolant": {
          "name": "diffusion-V"
        },
        "stochasticity": 1.0,
        "noise stddev": "correct",
        "steps skip type": {
          "name": "uniform",
          "K": 1
        },
        "schedule": {
          "name": "discrete",
          "beta schedule": "cosine",
          "timesteps": 100
        }
      }
    }
  }
}